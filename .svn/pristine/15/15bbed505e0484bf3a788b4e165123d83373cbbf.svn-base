package com.neusoft.entity;

import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

public class SellOutput {

	private Integer cusno;
	private String cusname;
	private Integer selloutid;
	@DateTimeFormat(pattern="yyyy-MM-dd")
	private Date salesdate;
	private Integer salesmanid;
	private String shaddress;
	private Integer totalamount;
	private Integer comno;
	private Integer amount;
	private Integer unitprice;
	
	public SellOutput() {
		super();
	}

	public SellOutput(Integer cusno, String cusname, Integer selloutid,
			Date salesdate, Integer salesmanid, String shaddress,
			Integer totalamount, Integer comno, Integer amount,
			Integer unitprice) {
		super();
		this.cusno = cusno;
		this.cusname = cusname;
		this.selloutid = selloutid;
		this.salesdate = salesdate;
		this.salesmanid = salesmanid;
		this.shaddress = shaddress;
		this.totalamount = totalamount;
		this.comno = comno;
		this.amount = amount;
		this.unitprice = unitprice;
	}

	public Integer getCusno() {
		return cusno;
	}

	public void setCusno(Integer cusno) {
		this.cusno = cusno;
	}

	public String getCusname() {
		return cusname;
	}

	public void setCusname(String cusname) {
		this.cusname = cusname;
	}

	public Integer getSelloutid() {
		return selloutid;
	}

	public void setSelloutid(Integer selloutid) {
		this.selloutid = selloutid;
	}

	public Date getSalesdate() {
		return salesdate;
	}

	public void setSalesdate(Date salesdate) {
		this.salesdate = salesdate;
	}

	public Integer getSalesmanid() {
		return salesmanid;
	}

	public void setSalesmanid(Integer salesmanid) {
		this.salesmanid = salesmanid;
	}

	public String getShaddress() {
		return shaddress;
	}

	public void setShaddress(String shaddress) {
		this.shaddress = shaddress;
	}

	public Integer getTotalamount() {
		return totalamount;
	}

	public void setTotalamount(Integer totalamount) {
		this.totalamount = totalamount;
	}

	public Integer getComno() {
		return comno;
	}

	public void setComno(Integer comno) {
		this.comno = comno;
	}

	public Integer getAmount() {
		return amount;
	}

	public void setAmount(Integer amount) {
		this.amount = amount;
	}

	public Integer getUnitprice() {
		return unitprice;
	}

	public void setUnitprice(Integer unitprice) {
		this.unitprice = unitprice;
	}

	@Override
	public String toString() {
		return "SellOutput [cusno=" + cusno + ", cusname=" + cusname
				+ ", selloutid=" + selloutid + ", salesdate=" + salesdate
				+ ", salesmanid=" + salesmanid + ", shaddress=" + shaddress
				+ ", totalamount=" + totalamount + ", comno=" + comno
				+ ", amount=" + amount + ", unitprice=" + unitprice + "]";
	}
	
	
	
}
