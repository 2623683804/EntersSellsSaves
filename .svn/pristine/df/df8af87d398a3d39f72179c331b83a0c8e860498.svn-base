package com.neusoft.entity;

import java.util.Date;

import org.springframework.format.annotation.DateTimeFormat;

public class PurReturn {

	private Integer supno;
	private Integer puroutid;
	private String supallname;
	@DateTimeFormat(pattern="yyyy-MM-dd")
	private Date thdate;
	private Integer totalamount;
	private Integer comno;
	private Integer amount;
	private Integer unitprice;
	
	public PurReturn() {
		super();
	}

	public PurReturn(Integer supno, Integer puroutid, String supallname,
			Date thdate, Integer totalamount, Integer comno, Integer amount,
			Integer unitprice) {
		super();
		this.supno = supno;
		this.puroutid = puroutid;
		this.supallname = supallname;
		this.thdate = thdate;
		this.totalamount = totalamount;
		this.comno = comno;
		this.amount = amount;
		this.unitprice = unitprice;
	}

	public Integer getSupno() {
		return supno;
	}

	public void setSupno(Integer supno) {
		this.supno = supno;
	}

	public Integer getPuroutid() {
		return puroutid;
	}

	public void setPuroutid(Integer puroutid) {
		this.puroutid = puroutid;
	}

	public String getSupallname() {
		return supallname;
	}

	public void setSupallname(String supallname) {
		this.supallname = supallname;
	}

	public Date getThdate() {
		return thdate;
	}

	public void setThdate(Date thdate) {
		this.thdate = thdate;
	}

	public Integer getTotalamount() {
		return totalamount;
	}

	public void setTotalamount(Integer totalamount) {
		this.totalamount = totalamount;
	}

	public Integer getComno() {
		return comno;
	}

	public void setComno(Integer comno) {
		this.comno = comno;
	}

	public Integer getAmount() {
		return amount;
	}

	public void setAmount(Integer amount) {
		this.amount = amount;
	}

	public Integer getUnitprice() {
		return unitprice;
	}

	public void setUnitprice(Integer unitprice) {
		this.unitprice = unitprice;
	}

	@Override
	public String toString() {
		return "PurReturn [supno=" + supno + ", puroutid=" + puroutid
				+ ", supallname=" + supallname + ", thdate=" + thdate
				+ ", totalamount=" + totalamount + ", comno=" + comno
				+ ", amount=" + amount + ", unitprice=" + unitprice + "]";
	}
	
}
