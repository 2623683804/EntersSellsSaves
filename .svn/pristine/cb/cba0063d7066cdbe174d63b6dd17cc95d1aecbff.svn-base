$(function(){
	//录入
	$("#add").click(function(){
		$("#div_entering").css("display","block");
	});
	
	$("#submit").click(function(){
		$("#div_entering").css("display","none");
	});
	
	$("#cancel").click(function(){
		$("#div_entering").css("display","none");
	});
	
	//修改
	$("#set").click(function(){
		$("#div_set").css("display","block");
	});
	
	$("#submit_01").click(function(){

		$("#div_set").css("display","none");
	});
	
	$("#cancel_01").click(function(){
		$("#div_set").css("display","none");
	});
	
	
});